---
import type { CollectionEntry } from 'astro:content';
interface Props {
  talks: CollectionEntry<'talks'>
}

const { talks } = Astro.props;
---

<h1>Talks</h1>
{
  talks.data.talks.map((year) => (
    <section class="prose-ul:list-none">
      <h3 class="prose-indigo">{year.year}</h3>
      <ul>
        {year.conferences.map((conference) => (
          <li>
            <a href="{conference.video}">{conference.title}</a>
            <br />
            {conference.conference}
          </li>
        ))}
      </ul>
    </section>
  ))
}
